package trabalhoAvaliativo;

public class Marca {
	
	// Atributos
	
	private int codigo;
	private String nome;
	
	// Getter e setters

	public int getCodigo() {
		return codigo;
	}
	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	
}
